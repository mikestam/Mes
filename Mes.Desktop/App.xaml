        <ResourceDictionary.MergedDictionaries>
            <ResourceDictionary ResourceDictionary.Source="pack://application:,,,/Mes.UI;component/Assets/Styles.xaml" />
            <ResourceDictionary ResourceDictionary.Source="pack://application:,,,/Mes.UI;component/Assets/Buttons.xaml" />
            <ResourceDictionary ResourceDictionary.Source="pack://application:,,,/Mes.UI;component/Assets/Controls.xaml" />
            <ResourceDictionary ResourceDictionary.Source="pack://application:,,,/Mes.UI;component/Assets/Paths.xaml" />
        </ResourceDictionary.MergedDictionaries>

    </Application.Resources>
</Application>
